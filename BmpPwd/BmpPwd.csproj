<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2.0.0</Version>
    <Company>mrousavy</Company>
    <Authors>mrousavy</Authors>
    <Description>A Library for en/decrypting text to Images</Description>
    <PackageProjectUrl>https://github.com/mrousavy/BmpPwd</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/mrousavy/BmpPwd/blob/master/LICENSE</PackageLicenseUrl>
    <Copyright>mrousavy</Copyright>
    <PackageIconUrl>https://raw.githubusercontent.com/mrousavy/BmpPwd/master/Images/Logo.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/mrousavy/BmpPwd</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageTags>encryption, decryption, cipher, image, bitmap, password, salt, hash, aes</PackageTags>
    <PackageReleaseNotes>.NET Standard Package</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="4.7.0" />
  </ItemGroup>

</Project>
